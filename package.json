{
  "name": "awesome-note-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk --loader tsx node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "tsx ./node_modules/typeorm/cli.js -d src/database/data-source.ts",
    "db:create": "tsx ./node_modules/typeorm-extension/dist/cli/index.js db:create -r src/database -d data-source.ts -f data-source.ts",
    "db:drop": "yarn typeorm schema:drop",
    "db:generate": "yarn typeorm migration:generate src/database/migrations/migrations",
    "db:run": "yarn typeorm migration:run",
    "db:rollback": "yarn typeorm migration:revert",
    "db:seed": "tsx ./node_modules/typeorm-extension/dist/cli/index.js seed -r src/database -d data-source.ts -f data-source.ts",
    "db:init": "yarn db:create && yarn db:generate && yarn db:run"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/bull": "0.6.1",
    "@nestjs/common": "9.1.2",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.2",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.1.2",
    "@nestjs/swagger": "6.1.3",
    "bcryptjs": "2.4.3",
    "bull": "4.10.1",
    "cache-manager": "4.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "dayjs": "1.11.6",
    "express-basic-auth": "1.2.1",
    "dotenv": "16.0.2",
    "ms": "2.1.3",
    "nestjs-i18n": "9.2.1",
    "nodemailer": "6.8.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6"    
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.3",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.2",
    "@nestjs/typeorm": "9.0.1",
    "@types/bcryptjs": "2.4.2",
    "@types/bull": "4.10.0",
    "@types/cache-manager": "4.0.2",
    "@types/express": "4.17.14",
    "@types/jest": "28.1.4",
    "@types/lodash": "4.14.187",
    "@types/node": "16.0.0",
    "@types/nodemailer": "6.4.6",
    "@types/passport-jwt": "3.0.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.0.3",
    "pg": "8.8.0",
    "prettier": "2.7.1",
    "source-map-support": "0.5.21",
    "supertest": "6.2.4",
    "ts-jest": "29.0.1",
    "ts-loader": "9.4.1",
    "tsconfig-paths": "4.1.0",
    "tsx": "3.9.0",
    "typeorm": "0.3.10",
    "typeorm-extension": "2.1.10",
    "typescript": "4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
